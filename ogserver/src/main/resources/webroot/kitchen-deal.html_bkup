<script>
    $(document).ready(function () {

        var offset = 400;
        var duration = 500;
        $(window).scroll(function () {
            if ($(this).scrollTop() > offset) {
                $('.back-to-contact').fadeIn(duration);
            } else {
                $('.back-to-contact').fadeOut(duration);
            }

        });


        $('.back-to-contact').click(function (event) {
            event.preventDefault();
            $('html, body').animate({scrollTop: 0}, duration);
            return false;
        });
    });

</script>

